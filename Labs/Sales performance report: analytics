    "#customers growth:\n",
    "new_customers_2003 = df_sales[(df_sales.year_id == 2003)].customername.unique().tolist()\n",
    "customers_2004 = df_sales[(df_sales.year_id == 2004) & (df_sales.year_id == 2004)].customername.unique().tolist()\n",
    "new_customers_2004 = [x for x in customers_2004 if x not in new_customers_2003] \n",
    "customers_2005 = df_sales[(df_sales.year_id == 2005)].customername.unique().tolist()\n",
    "new_customers_2005 = [x for x in customers_2005 if x not in customers_2004]\n",
    "#customers churn: \n",
    "#2004:\n",
    "customers_2003 = df_sales[df_sales.year_id == 2003].customername.unique().tolist()\n",
    "customers_2004_2005 = df_sales[df_sales.year_id > 2004].customername.unique().tolist()\n",
    "lost_customers_2004 = [x for x in customers_2003 if x not in customers_2004_2005]\n",
    "#2005:\n",
    "customers_2004 = df_sales[df_sales.year_id == 2004].customername.unique().tolist()\n",
    "customers_2005 = df_sales[df_sales.year_id == 2005].customername.unique().tolist()\n",
    "lost_customers_2005 = [x for x in customers_2004 if x not in customers_2005]\n",
    "acquisition = (69, 23, 8)\n",
    "lost = (0, 39, 52)\n",
    "fig, ax = plt.subplots()\n",
    "index = np.arange(3)\n",
    "bar_width = 0.35\n",
    "opacity = 0.8\n",
    "rects1 = plt.bar(index, acquisition, bar_width, alpha=opacity, color='tab:green', label='Acquisition')\n",
    "rects2 = plt.bar(index + bar_width, lost, bar_width, alpha=opacity, color='tab:red', label='Lost')\n",
    "plt.xlabel('Year')\n",
    "plt.ylabel('Number of customers')\n",
    "plt.title('Customers flow (Acquisition vs Lost), 2003-2004')\n",
    "plt.xticks(index + bar_width, ('2003', '2004', '2005'))\n",
    "plt.legend()\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
