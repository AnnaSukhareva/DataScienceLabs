    "y_pos = np.arange(len(objects))\n",
    "colors = ['tab:green', 'tab:gray', 'tab:blue', 'tab:olive', 'tab:purple', 'tab:red']\n",
    "plt.bar(y_pos, performance, align='center', alpha=0.5, color=colors)\n",
    "plt.xticks(y_pos, objects)\n",
    "plt.ylabel('Number of commodities')\n",
    "plt.title('Status of orders')\n",
    "plt.show()"
   ]
